    "eslint": "^7.32.0",
    "eslint-config-custom": "workspace:*",
    "tsconfig": "workspace:*",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.4.9",
    "@dxp/utils": "workspace:*",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "framer-motion": "^8.5.0",
    "react-datepicker": "^4.10.0",
    "react-hook-form": "^7.43.0",
    "react-number-format": "^5.1.3",
    "react-select": "^5.7.0"
