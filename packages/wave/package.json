{
  "name": "@vastly/wave",
  "version": "0.2.4",
  "description": "Vastly's Wave Package",
  "author": "Vastly",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/companydotcom/vastly",
    "directory": "packages/wave"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "engines": {
    "node": ">=14.16"
  },
  "bin": {
    "wave": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup && pnpm copy-files",
    "dev": "ts-node --esm ./src/index.ts",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "copy-files": "cp -R ./src/templates/ ./dist/templates"
  },
  "sideEffects": [
    "dist/*"
  ],
  "dependencies": {
    "@sentry/node": "^7.48.0",
    "@vastly/utils": "workspace:*",
    "ansi-escapes": "^6.2.0",
    "bundle-require": "4.0.1",
    "chalk": "^4.1.2",
    "commander": "^10.0.0",
    "email-validator": "^2.0.4",
    "fast-glob": "^3.2.12",
    "find-up": "^6.3.0",
    "fs-extra": "^11.1.0",
    "inquirer": "^9.1.5",
    "joycon": "3.1.1",
    "js-yaml": "^4.1.0",
    "load-json-file": "^7.0.1",
    "node-fetch": "^3.3.1",
    "ora": "^6.3.0",
    "strip-json-comments": "5.0.1",
    "type-fest": "^3.10.0",
    "write-json-file": "^5.0.0",
    "xdg-app-paths": "^8.3.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^9.0.3",
    "@types/js-yaml": "^4.0.5",
    "@types/load-json-file": "^5.1.0",
    "@types/node-fetch": "^2.6.3",
    "@types/write-json-file": "^3.2.1",
    "@vastly/eslint-config-custom": "workspace:*",
    "@vastly/types": "workspace:*",
    "eslint": "^7.32.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vitest": "0.31.0",
    "vitest-fetch-mock": "^0.2.2"
  },
  "keywords": [
    "vastly",
    "wave"
  ]
}
