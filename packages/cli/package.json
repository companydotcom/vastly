{
  "name": "cli",
  "version": "0.0.1",
  "description": "Company.com CLI",
  "author": "Company.com",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "tsup": {
    "dts": true,
    "bundle": false,
    "treeshake": true
  },
  "engines": {
    "node": ">=14.16"
  },
  "bin": {
    "vastly": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --format esm,cjs src/index.ts",
    "dev": "ts-node --esm ./src/index.ts",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "commander": "^10.0.0",
    "inquirer": "^9.1.5",
    "load-json-file": "^7.0.1",
    "ora": "^6.3.0",
    "write-json-file": "^5.0.0",
    "xdg-app-paths": "^8.3.0"
  },
  "devDependencies": {
    "@companydotcom/types": "workspace:*",
    "@companydotcom/utils": "workspace:*",
    "@octokit/rest": "^19.0.7",
    "@sentry/node": "^7.48.0",
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^9.0.3",
    "@types/load-json-file": "^5.1.0",
    "@types/node-fetch": "^2.6.3",
    "@types/write-json-file": "^3.2.1",
    "eslint": "^7.32.0",
    "eslint-config-custom": "workspace:*",
    "fs-extra": "^11.1.0",
    "node-fetch": "^3.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}